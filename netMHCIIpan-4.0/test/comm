../netMHCIIpan -inptype 1 -f example.pep_context -context > example.pep_context.out
../netMHCIIpan -inptype 1 -f example.pep_context -context -BA > example.pep_context_BA.out

../netMHCIIpan -inptype 0 -f example.fsa > example.fsa.out
../netMHCIIpan -inptype 0 -f example.fsa -context > example.fsa_context.out
../netMHCIIpan -f example.fsa -a H-2-IAb -s -u > example.fsa.sorted.out

